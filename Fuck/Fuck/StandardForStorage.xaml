<Window x:Class="Fuck.StandardForStorage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Fuck"
        mc:Ignorable="d"
        Title="StandardForStorage" Height="450" Width="800">
    <Grid>
        <ListBox x:Name="Products" HorizontalAlignment="Left" Height="346" Margin="10,10,0,0" VerticalAlignment="Top" Width="217" SelectionChanged="Products_SelectionChanged"/>
        <TextBox x:Name="Quantity" HorizontalAlignment="Left" Height="23" Margin="306,36,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="Колличество" HorizontalAlignment="Left" Margin="306,10,0,0" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="Change" Content="Change" HorizontalAlignment="Left" Height="36" Margin="306,59,0,0" VerticalAlignment="Top" Width="120" Click="Change_Click"/>
        <Button x:Name="Aply" Content="Aply" HorizontalAlignment="Left" Height="49" Margin="306,307,0,0" VerticalAlignment="Top" Width="120" Click="Aply_Click"/>

    </Grid>
</Window>
